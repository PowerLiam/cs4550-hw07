<div class="row">
  <div class="col">
    <h1>Events Feed</h1>
  </div>
</div>

<div class="row">
  <%= for post <- @posts do %>
    <div class="col-sm-4 card">
      <div class="card-body">
        <p class="card-text"><%= event.name %></p>
        <%= link "view", to: Routes.event_path(@conn, :show, event) %>
      </div>
    </div>
  <% end %>
</div>